{
  "name": "rentitforward-mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "type-check": "tsc --noEmit",
    "build:shared": "cd ../rentitforward-shared && npm run build && robocopy dist ../rentitforward-mobile/shared-dist /E /NFL /NDL /NJH /NJS /nc /ns /np | cmd /c exit 0",
    "dev": "npm run build:shared && expo start",
    "dev:clear": "npm run build:shared && expo start --clear",
    "build:prod": "npm run build:shared && expo export",
    "reset": "expo r",
    "tunnel": "expo start --tunnel"
  },
