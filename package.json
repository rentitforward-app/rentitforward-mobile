{
  "name": "rentitforward-mobile",
  "version": "1.0.0",
  "main": "expo-router/entry",
  "scripts": {
    "start": "expo start",
    "android": "expo run:android",
    "ios": "expo run:ios",
    "web": "expo start --web",
    "lint": "expo lint",
    "type-check": "tsc --noEmit",
    "build:shared": "cd ../rentitforward-shared && npm run build && robocopy dist ../rentitforward-mobile/shared-dist /E /NFL /NDL /NJH /NJS /nc /ns /np | cmd /c exit 0",
    "dev": "npm run build:shared && expo start",
    "dev:clear": "npm run build:shared && expo start --clear",
    "build:prod": "npm run build:shared && expo export",
    "reset": "expo r",
    "tunnel": "expo start --tunnel"
  },
  "dependencies": {
    "@apollo/client": "^3.13.8",
    "@expo/vector-icons": "^15.0.2",
    "@gorhom/bottom-sheet": "^5.1.6",
    "@hookform/resolvers": "^5.1.1",
    "@react-native-async-storage/async-storage": "2.2.0",
    "@react-native-community/datetimepicker": "8.4.4",
    "@react-native-community/slider": "5.0.1",
    "@react-native-picker/picker": "2.11.1",
    "@react-navigation/native": "^7.0.14",
    "@sentry/react-native": "~6.20.0",
    "@stripe/stripe-react-native": "0.50.3",
    "@supabase/supabase-js": "^2.39.0",
    "@tanstack/react-query": "^5.82.0",
    "date-fns": "^4.1.0",
    "expo-calendar": "~15.0.7",
    "expo-constants": "~18.0.8",
    "expo-document-picker": "~14.0.7",
    "expo-font": "~14.0.8",
    "expo-image-manipulator": "~14.0.7",
    "expo-image-picker": "~17.0.8",
    "expo-linking": "~8.0.8",
    "expo-location": "~19.0.7",
    "expo-router": "~6.0.4",
    "expo-splash-screen": "~31.0.10",
    "expo-status-bar": "~3.0.8",
    "expo-system-ui": "~6.0.7",
    "expo-updates": "~29.0.10",
    "graphql": "^16.11.0",
    "nativewind": "^2.0.11",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "react-hook-form": "^7.60.0",
    "react-native": "0.81.4",
    "react-native-calendars": "^1.1313.0",
    "react-native-crypto": "^2.2.1",
    "react-native-encrypted-storage": "^4.0.3",
    "react-native-gesture-handler": "~2.28.0",
    "react-native-keychain": "^10.0.0",
    "react-native-randombytes": "^3.6.2",
    "react-native-reanimated": "~4.1.0",
    "react-native-safe-area-context": "~5.6.0",
    "react-native-screens": "~4.16.0",
    "react-native-svg": "15.12.1",
    "react-native-svg-transformer": "^1.5.1",
    "react-native-web": "^0.21.0",
    "react-native-worklets": "^0.5.1",
    "tailwindcss": "^3.4.17",
    "zod": "^4.0.2",
    "zustand": "^5.0.6",
    "react-native-webview": "13.15.0"
  },
  "devDependencies": {
    "@babel/core": "^7.25.9",
    "@types/react": "~19.1.10",
    "babel-plugin-module-resolver": "^5.0.2",
    "eslint": "^8.57.1",
    "eslint-config-expo": "~10.0.0",
    "expo": "^54.0.7",
    "typescript": "~5.9.2"
  },
  "overrides": {
    "react-native-safe-area-context": "~5.6.0"
  },
  "expo": {
    "doctor": {
      "reactNativeDirectoryCheck": {
        "exclude": [
          "react-native-encrypted-storage"
        ]
      }
    }
  },
  "private": true
}
